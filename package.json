{
  "name": "react-pink",
  "version": "1.0.0",
  "description": "portfolio project",
  "main": "index.js",
  "scripts": {
    "start": "node build/bundle.js",
    "client": "npm run start --prefix client",
    "dev": "npm-run-all --parallel  dev:*",
