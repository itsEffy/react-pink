    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-flow": "2.29.1",
    "eslint-plugin-flowtype": "2.33.0",
    "prettier": "^1.13.7",
    "flow-bin": "^0.72.0",
    "flow-typed": "^2.4.0"
  },
  "dependencies": {
    "axios": "0.18.0",
